MODULE_NAME="Length of lines"
KEYWORDS="ALL"

LEVEL="BASIC"

exec_module() {
	UTFCHECKER="check-utf8"

	UTF=$(${EXTRA_DIR}/${UTFCHECKER} ${DOC} 2>&1)

	if [[ -z ${UTF} ]]; then
		OUTPUT_RESULT="ok"
	else
		OUTPUT_RESULT="critical"
		OUTPUT_TEXT="${UTF}"
	fi
}
