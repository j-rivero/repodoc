MODULE_NAME="Length of lines"
KEYWORDS="*"

LEVEL="BASIC"

exec_module() {
	UTFCHECKER="check-utf8"

	UTF=$(${EXTRA_DIR}/${UTFCHECKER} ${DOC} 2>&1)

	if [[ -z ${UTF} ]]; then
		return 0
	else
		OUTPUT_TEXT="${UTF}"
		return 2
	fi
}
